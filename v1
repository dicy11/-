#include "stdio.h"
#include "string.h"
int main()
{
	char str[]="1+2+2+1+2+5+4-1-3+4-8";
	int a,i;
	int ans=str[0]-'0';
	for(i=1;i<strlen(str);i++)
	{
		if(str[i]=='+')
		{
			a=str[i+1]-'0';
			ans=ans+a;
			i++;
		}
		else if(str[i]=='-')
		{
			a=str[i+1]-'0';
			ans=ans-a;
			i++;
		}
	 } 
	 printf("%d\n",ans);
	return 0;
 } 
